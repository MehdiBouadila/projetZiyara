{% extends 'back.html.twig' %}

{% block title %}Pack index{% endblock %}

{% block css %}
 {{ parent() }} 
{% endblock %}



{% block body %}

{{ parent() }} 
    <h1>Pack index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Titre_Pack</th>
                <th>Description_Pack</th>
                <th>Prix_Pack</th>
                <th>Date_Pack</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for pack in packs %}
            <tr>
                <td>{{ pack.id }}</td>
                <td>{{ pack.titrePack }}</td>
                <td>{{ pack.descriptionPack }}</td>
                <td>{{ pack.prixPack }}</td>
                <td>{{ pack.datePack }}</td>
                <td>
                    <a href="{{ path('app_pack_show', {'id': pack.id}) }}">show</a>
                    <a href="{{ path('app_pack_edit', {'id': pack.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="7">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_pack_new') }}">Create new</a>
{% endblock %}
