<form method="post" action="{{ path('app_client_delete', {'idClient': client.idClient}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ client.idClient) }}">
    <div class="mt-3">
    <button class="btn btn-info">Delete</button>
    </div>
</form>
